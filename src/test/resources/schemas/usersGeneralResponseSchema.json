{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "meta": {
      "type": "null"
    },
    "data": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "gender": {
          "type": "string",
          "enum": ["male", "female"]
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "name",
        "email",
        "gender",
        "status"
      ]
    }
  },
  "required": [
    "data"
  ]
}